//functions in javascript 

//syntax function functionname(parametes)
//function to print the given input 
function print(x){

    console.log(x);

}

print("vignesh") //prints the vighnesh as output 


//short inline functions 
//this fucntions can be used with the keyword fucntion  

const print2=(y)=>{console.log(y);}

print2("vamsi") //prints the vamsi as output